{
  "extends": "../../tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "paths": {
      "@analysis/*": ["process/src/analysis/*"],
      "@generator/*": ["process/src/generator/*"],
      "@pipeline/*": ["process/src/pipeline/*"],
      "@transform/*": ["process/src/transform/*"],
      "@util/*": ["process/src/util/*"]
    }
  }
}